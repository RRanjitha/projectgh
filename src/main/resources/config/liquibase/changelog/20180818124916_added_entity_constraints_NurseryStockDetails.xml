                                 constraintName="fk_nursery_stock_details_sapling_damage_area_id"
                                 referencedColumnNames="id"
                                 referencedTableName="pick_list_value"/>

        <addForeignKeyConstraint baseColumnNames="financial_year_stock_details_id"
